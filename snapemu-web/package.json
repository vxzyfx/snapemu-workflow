    "prettier": "^3.0.3",
    "sass": "^1.69.6",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vitest": "^1.0.4",
    "vue-tsc": "^1.8.25"
